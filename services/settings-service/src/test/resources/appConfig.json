  "sslCertificate": "",
  "services": {
    "main": {
      "name": "main",
      "description": "Main Service",
      "public": true,
      "path": "/*",
      "database": {
        "engine": "MYSQL",
        "version": "1.2.3",
        "family": "t3.medium",
        "database": "example",
        "username": "example",
        "password": "examplepass",
        "bootstrapFilename": null,
        "passwordParam": "exampleParam",
        "tiers": {
          "Free": {
            "instance": "db.t3.micro"
          },
          "Gold": {
            "instance": "db.t3.xlarge"
          }
        }
      },
      "compute": {
        "type": "ECS",
        "ecsExecEnabled": true,
        "healthCheckUrl": "/health",
        "operatingSystem": "LINUX",
        "containerPort": 7000,
        "containerRepo": "",
        "containerTag": "latest",
        "tiers": {
          "default": {
            "computeSize": "S",
            "min": 1,
            "max": 2,
            "ec2min": 1,
            "ec2max": 2
          }
        }
      },
      "filesystem": {
        "type": "EFS",
        "mountPoint": "/mnt",
        "tiers": {
          "Free": {
            "encrypt": false,
            "lifecycle": "NEVER"
          },
          "Gold": {
            "encrypt": true,
            "lifecycle": "AFTER_7_DAYS",
            "encryptionKey": "encryptionARN"
          }
        }
      }
    },
